{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/contact", "dest": "/contact.html" },
    { "src": "/projects/(.*)", "dest": "/projects/$1.html" },
    { "src": "/(.*)", "dest": "/404.html" }
  ]
}
